{
  "name": "mock-server",
  "version": "0.0.1",
  "description": "Use restify to build a mock server for dev.",
  "main": "server.js",
  "repository": "https://github.com/vivihuang/mock-server.git",
  "author": "Vivi Huang",
  "license": "none",
  "ava": {
    "files": [
      "**/*.spec.js"
    ],
    "source": [
      "**/*.{js}",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  },
  "scripts": {
    "start": "NODE_PATH=. nodemon server.js --exec babel-node",
    "test": "ava"
  },
  "dependencies": {
    "app-module-path": "^2.2.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "faker": "^4.1.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "nodemon": "^1.18.6",
    "require-dir": "^1.1.0",
    "restify": "^7.2.2",
    "restify-cors-middleware": "^1.1.1"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "es6-promise": "^4.2.5",
    "isomorphic-fetch": "^2.2.1"
  }
}
